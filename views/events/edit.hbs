<h1>{{title}}</h1>
<main>

	<h2>Edit Events</h2>

	<a href="/">🏠</a>

	<div id="edit-form" class="user-container-show">

    <form action="/{{user._id}}?_method=PATCH" method="POST">
      <div>
        <label for="name">name: </label>
        <input type="text" name="name" value="{{user.name}}">
      </div>

      <div>
        <label for="img">img: </label>
        <input type="text" name='img' value="{{user.img}}">
      </div>

   <a href="">  <button type="submit">Create New Event</button></a> 
    </form>



		<div id="img-preview">
			<img src="{{user.img}}">
		</div>

	</div>
</main>